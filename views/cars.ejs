<h2>Cars</h2>

<h3>Search Results for <%= search %> </h3>

<% carImg.forEach((c) => { %>
    <img src=<%= c.dataValues.image %> alt="<%= c.dataValues.model %>">
    <p><%= c.dataValues.model %></p>
    <form action="cars/fav" method="POST" id="carsfavform"> 
    <input type="hidden" id="favecar_model" name="favecar_model" value="<%= c.dataValues.model %>"/>
    <input type="hidden" id="favecar_make" name="favecar_make" value="<%= c.dataValues.make %>"/>
    <input type="hidden" id="carimg" name="carimg" value="<%= c.dataValues.image %>"/>
    <input type="hidden" id="userId" name="userId" value="<%= //currentUser.id %>"/>
    <button type="submit">Add to Favorites</button>
    </form>
<% }) %>  